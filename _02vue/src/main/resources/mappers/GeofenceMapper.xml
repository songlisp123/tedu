<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com._02vue.demo.geofence.mapper.GeofenceMapper">
    <select id="query" resultType="com._02vue.demo.geofence.pojo.vo.GeofenceVo">
        SELECT id,name,position,create_time as createTime,status
        FROM geofence
        <where>
            <if test=" id != null ">
               AND id =#{id}
            </if>
            <if test="name != null and name != ''">
                AND name = #{name}
            </if>
            <if test="status != null and status != ''">
                AND status = #{status}
            </if>
        </where>
    </select>
</mapper>
