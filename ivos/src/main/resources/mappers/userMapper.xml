<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ivos.demo.user.mapper.userMapper">
    <select id="selectUserByUserNameAndPassword" resultType="com.ivos.demo.user.pojo.vo.UserVo">
        SELECT id,username,password,email,phone,age,gender,create_time,update_time,status,level,parent_id
        FROM user WHERE username=#{username}
    </select>
</mapper>