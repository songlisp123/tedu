<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egadress0904.demo.mapper.adressMapper">
    <insert id="insertAddress">
        INSERT INTO address(
        receiver,
        address,
        email,
        mobile,
        tag
        )
        VALUES(
        #{receiver},
        #{address},
        #{email},
        #{mobile},
        #{tag}
        )
    </insert>
</mapper>
